using System;

    class Program
    {
        //Write an if-statement that takes two double variables a and b and exchanges their values if
        //the first one is greater than the second one. As a result print the values a and b, separated by a space.
        static void Main()
        {
            Console.WriteLine("Enter your first double variable: ");
            double numberA = double.Parse(Console.ReadLine());
            Console.WriteLine("Enter your second double variable: ");
            double numberB = double.Parse(Console.ReadLine());

            if (numberA > numberB)
            {
                double temp = numberA;
                numberA = numberB;
                numberB = temp;
            }
            Console.WriteLine(numberA + " " + numberB);
        }
    }

